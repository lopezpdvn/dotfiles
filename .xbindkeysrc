# Use `xbindkeys -k` to find the keycodes for a particular key

"quodlibet --seek=+00:15"
    Control+Mod2+Mod4 + KP_Right

"quodlibet --seek=-00:15"
    Control+Mod2+Mod4 + KP_Left

"quodlibet --seek=+00:05"
    Control+Mod2+Mod4 + KP_Next

"quodlibet --seek=-00:05"
    Control+Mod2+Mod4 + KP_End

"quodlibet --seek=+00:30"
    Control+Mod2+Mod4 + KP_Prior

"quodlibet --seek=-00:30"
    Control+Mod2+Mod4 + KP_Home

"quodlibet --next"
    Control+Mod2+Mod4 + KP_Up

"quodlibet --previous"
    Control+Mod2+Mod4 + KP_Down

"quodlibet --play-pause"
    Control+Mod2+Mod4 + KP_Enter
